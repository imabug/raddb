{
    "name": "imabug/raddb",
    "description": "Radiology Equipment Database.",
    "keywords": [
        "laravel",
        "radiology",
        "inventory",
        "equipment"
    ],
    "homepage": "https://github.com/imabug/raddb",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.2",
        "laravel/dusk": "^6.14",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "nesbot/carbon": "^2.0",
        "phpoffice/phpspreadsheet": "^1.9",
        "spatie/laravel-medialibrary": "^7.5",
        "symfony/yaml": "^4.2",
        "twbs/bootstrap": "^3.4"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.13",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "RadDB\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
