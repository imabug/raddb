{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "consoletvs/charts": "^5.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "laravel/dusk": "^2.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.5",
        "nesbot/carbon": "^1.22",
        "phpoffice/phpspreadsheet": "^1.0",
        "spatie/laravel-artisan-dd": "^2.0",
        "spatie/laravel-backup": "^5.0.0",
        "spatie/laravel-medialibrary": "^6.0.0",
        "symfony/yaml": "^3.3",
        "tcg/voyager": "^1.0",
        "twbs/bootstrap": "^3.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "RadDB\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
