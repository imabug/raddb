{
    "name": "imabug/raddb",
    "description": "Radiology Equipment Database.",
    "keywords": [
        "laravel",
        "radiology",
        "inventory",
        "equipment"
    ],
    "homepage": "https://github.com/imabug/raddb",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Eugene Mah",
            "email": "eugenemah@gmail.com",
            "homepage": "https://v16.imablog.net/"
        }
    ],
    "support": {
        "email": "eugenemah@gmail.com",
        "issues": "https://github.com/imabug/raddb/issues",
        "source": "https://github.com/imabug/raddb",
        "wiki": "https://github.com/imabug/raddb/wiki",
        "docs": "https://github.com/imabug/raddb/wiki"
    },
    "require": {
        "php": "^8.0.2",
        "doctrine/dbal": "^3.4.4",
        "guzzlehttp/guzzle": "^7.5",
        "khill/lavacharts": "^3.1.14",
        "laravel/framework": "^9.28",
        "laravel/sanctum": "^3.0.1",
        "laravel/tinker": "^2.7.2",
        "laravel/ui": "^4.0.1",
        "livewire/livewire": "^2.10.7",
        "nesbot/carbon": "^2.62.1",
        "phpoffice/phpspreadsheet": "^1.24.1",
        "rappasoft/laravel-livewire-tables": "^2.8",
        "spatie/laravel-medialibrary": "^10.4.5",
        "symfony/yaml": "^6.1.4",
        "twbs/bootstrap": "^5.2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "laravel/dusk": "^7.0.1",
        "laravel/sail": "^1.16",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3",
        "pestphp/pest": "^1.22.1",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.24",
        "spatie/laravel-ignition": "^1.4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
},
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
